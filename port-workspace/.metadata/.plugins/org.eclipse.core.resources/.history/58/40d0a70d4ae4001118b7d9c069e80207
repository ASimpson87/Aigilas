package com.aigilas.entities;import com.spx.wrapper.*;import java.util.*;import ï»¿com.aigilas.creatures.*;import com.spx.entities.*;import com.spx.core.*;
    public class Extensions
    {
        public static ICreature IsCreature(this IEntity entity)
        {
            if (entity.GetEntityType() == com.aigilas.EntityType.ACTOR)
            {
                return entity as ICreature;
            }
